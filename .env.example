# NextAuth
AUTH_SECRET="your_nextauth_secret"

# Vercel Cron Secret (for scheduled syncs)
CRON_SECRET=your_cron_secret

# Trigger.dev
TRIGGER_SECRET_KEY=your_trigger_secret_key
